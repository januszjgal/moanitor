{
  "expo": {
    "name": "moanitor",
    "slug": "moanitor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/face.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/face.png",
        "backgroundColor": "#9333EA"
      },
      "package": "com.januszapps.moanitor"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/face.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#9333EA"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "703984de-9a24-47cf-a1ae-991a9bc5dfa7"
      }
    }
  }
}
